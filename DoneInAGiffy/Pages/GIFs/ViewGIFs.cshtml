@page
@model DoneInAGiffy.Pages.GIFs.ViewGIFsModel
@{
}
@foreach (var GIF in Model.GIF) {
    <div class="row m-2 border">
        <div class="row m-2">
            <div class="col-2">
                <label class="form-label" asp-for="@GIF.gifTitle"></label>
            </div>
            <div class="col-10">
                <label class="form-label">@GIF.gifTitle</label>
            </div>
        </div>
    </div>
    <div class="row m-2 border">
        <div class="row m-2">
            <div class="col-2">
                <a asp-page="EditGIF" asp-route-id="@GIF.gifID" class="btn btn-primary">Edit</a>
            </div>
        </div>
    </div>
}
